@model NeuropsychInventory.ViewModels.TakeInventoryVM

@{
    ViewBag.Title = "TakeInventory";
}
<table>
    @foreach (var item in Model.Tests)
    {
        <tr>
            <td>@Html.ActionLink(item.TestName, "TakeItemInventory", new { inventoryId = Model.InventoryId, testId = item.Id }, new {@class="mobile-link"})</td>
            <td>
                @if (item.IsComplete == true) {
                    <img style="vertical-align: middle" src="~/Images/greencheck.png" />
                }
                else {
                    <img style="vertical-align: middle" src="~/Images/red_x.gif" />
                }
            </td>
        </tr>
    }
</table>


<p />
@Html.ActionLink("Conclude", "ConcludeInventory")
<p />
@Html.ActionLink("Back to List", "Index")